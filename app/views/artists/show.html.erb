<h1><%= @artist.name %></h1>
<h2><%= @artist.title %></h2>
<p>Age: <%= @artist.age %></p>


<%if @artist.instruments != []%>
    <h3>Instruments:</h3>
    <ul>
        <%@artist.instruments.each do |i|%>
            <%= i.name %>
        <%end%>
    </ul>
<%end%>

<%= link_to 'Back to Index', artists_path %>